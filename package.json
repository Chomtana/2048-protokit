{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:local": "turbo run dev:local",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "migrate": "turbo run migrate",
    "start": "turbo run start",
    "docker:up": "docker-compose -f docker/docker-compose.yml up",
    "prisma:generate": "turbo run prisma:generate"
  },
  "devDependencies": {
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "tsconfig": "workspace:*",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.6.10",
  "name": "starter-kit"
}
